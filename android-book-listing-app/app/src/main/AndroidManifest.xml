<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.books">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".loginecadastro.CadastroActivity"/>
        <activity android:name=".livroutils.CadastroLivroActivity"/>
        <activity android:name=".ListarEmprestimosActivity"/>
        <activity android:name=".BuscaActivity"/>
        <activity android:name=".livroutils.LivrosActivity"/>
        <activity android:name=".loginecadastro.LoginActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>